<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">

    <Appenders>
        <Console name="CONSOLE" target="SYSTEM_OUT">
            <PatternLayout>
                <Pattern>%d |%-5level [%thread] %logger{120}:%L - %m%ex%n</Pattern>
            </PatternLayout>
        </Console>
    </Appenders>

    <Loggers>
        <Root level="debug">
            <appender-ref ref="CONSOLE"/>
        </Root>
    </Loggers>

</Configuration>